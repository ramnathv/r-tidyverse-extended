{
  "initial_working_directory": "~",
  "server_editor_font_enabled": true,
  "server_editor_font": "Fira Code",
  "save_workspace": "never",
  "load_workspace": false,
  "soft_wrap_r_files": true,
  "continue_comments_on_newline": true,
  "highlight_r_function_calls": true,
  "rainbow_parentheses": true,
  "tab_multiline_completion": true,
  "font_size_points": 11,
  "rmd_viewer_type": "pane",
  "editor_theme": "Dracula",
  "insert_native_pipe_operator": true,
  "panes": {
    "quadrants": ["Source", "TabSet1", "Console", "TabSet2"],
    "tabSet1": [
      "Environment",
      "History",
      "Connections",
      "Build",
      "VCS",
      "Tutorial",
      "Presentation"
    ],
    "tabSet2": [
      "Files",
      "Plots",
      "Packages",
      "Help",
      "Viewer",
      "Presentations"
    ],
    "hiddenTabSet": [],
    "console_left_on_top": false,
    "console_right_on_top": true,
    "additional_source_columns": 0
  },
  "python_type": "system",
  "python_version": "3.12.3",
  "python_path": "/usr/bin/python3.12",
  "git_diff_ignore_whitespace": true,
  "reformat_on_save": true,
  "code_formatter": "external",
  "code_formatter_external_command": "/usr/local/bin/air format",
  "posix_terminal_shell": "zsh"
}
